doctype html
html(ng-app='nhl')
  head
    meta(name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no')
    base(href='/')
    title= title
    link(rel='stylesheet' href='https://unpkg.com/bootstrap-material-design@4.1.1/dist/css/bootstrap-material-design.min.css')
    link(rel='stylesheet' href='/stylesheets/style.css')
    link(rel='stylesheet' href='https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Material+Icons')
    link(rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/bootstrap-slider/10.0.0/css/bootstrap-slider.min.css')
    link(rel='stylesheet' href='/stylesheets/jquery.bracket.min.css')
    script(src='https://code.jquery.com/jquery-3.2.1.min.js')
    script(src='javascripts/arrive.min.js')
    script(src='https://unpkg.com/popper.js@1.12.6/dist/umd/popper.js')
    script(src='https://unpkg.com/bootstrap-material-design@4.1.1/dist/js/bootstrap-material-design.js')
    script(src='https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular.min.js')
    script(src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-slider/10.0.0/bootstrap-slider.min.js")
    script(src='javascripts/nhl.js')
    script(src='javascripts/jquery.bracket.min.js')
    script(src='https://d3js.org/d3.v4.min.js')
  body.ng-cloak
    block content